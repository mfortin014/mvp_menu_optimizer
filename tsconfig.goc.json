{
  "compilerOptions": {
    "target": "ES2020",
    "module": "Node16",
    "moduleResolution": "Node16",
    "outDir": "dist/goc",
    "rootDir": "scripts/goc",
    "declaration": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": false,
    "types": ["node"]
  },
  "include": ["scripts/goc/**/*.ts"]
}
